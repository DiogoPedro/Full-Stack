{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "School registration",
    "properties": {
        "name": {
            "type":"string",
            "minLength": 10,
            "maxLength": 30
        },
        "age": {
            "type":"integer",
            "minimum": 1
        },
        "cpf":{
            "type":"integer",
            "minimum": 10000000000,
            "maximum": 99999999999
        },
        "allergy":{
            "type":"object"
        },
        "deficiency":{
            "type":"boolean"
        }
    },
    "required": ["name", "age", "cpf", "allergy"]
}